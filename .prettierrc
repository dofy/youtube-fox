{
  "jsxSingleQuote": false,
  "singleQuote": true,
  "trailingComma": "all",
  "endOfLine": "lf",
  "printWidth": 100,
  "semi": false,
  "tabWidth": 2,
  "useTabs": false,
  "plugins": ["prettier-plugin-css-order"],
  "cssOrder": [
    "position",
    "top",
    "right",
    "bottom",
    "left",
    "display",
    "flex",
    "align-items",
    "justify-content",
    "width",
    "height"
  ]
}
